# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8
max_line_lenght = 120
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
indent_size = 1
max_line_lenght = 80
trim_trailing_whitespace = false
